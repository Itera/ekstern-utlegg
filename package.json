{
  "name": "itera-external-expenses",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.23",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-helmet": "^5.0.8",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/reactstrap": "^7.1.3",
    "bootstrap": "^4.2.1",
    "moment": "^2.24.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-localstorage-simple": "^2.1.4",
    "serve": "^10.1.2",
    "typescript": "^3.3.3",
    "validate.js": "^0.12.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "yarn build && serve -s build",
    "package": "yarn build && docker build -t docker.mad.itera.no/itera/ekstern_utlegg:latest .",
    "run-package": "docker run -d -p5000:5000 --name utlegg docker.mad.itera.no/itera/ekstern_utlegg:latest",
    "format": "prettier 'src/**/*.{js,ts,jsx,tsx,css,md}' --write",
    "test:staged": "CI=true react-scripts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts,jsx,tsx,css,md}": [
        "yarn run format",
        "yarn run test:staged",
        "git add"
      ]
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "singleQuote": true
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "react-testing-library": "^5.5.3"
  }
}
